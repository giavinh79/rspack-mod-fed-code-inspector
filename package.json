    "@rspack/cli": "1.4.11",
    "@rspack/core": "1.4.1",
    "@rspack/plugin-react-refresh": "1.4.1",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "code-inspector-plugin": "^1.2.2",
    "react-refresh": "0.17.0"
  },
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  }
}
